{
    "name": "argprokeymap",
    "publisher": "AnkurGoyal",
    "displayName": "argprokeymap",
    "description": "this is a customised keymap by Ankur",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "keymap",
        "slickedit",
        "slickedit pro",
        "slickedit keymap",
        "argpro",
        "argprokeymap",
        "AnkurGoyal"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/argankur/argprokeymap.git"
    },
    "contributes": {
        "keybindings": [
            {
                "key": "shift+f8",
                "command": "highlightwords.addHighlight"
            },
            {
                "key": "f12",
                "command": "-editor.action.revealDefinition",
                "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
            },
            {
                "key": "f12",
                "command": "editor.action.revealDefinition",
                "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
            },
            {
                "key": "ctrl+oem_6",
                "command": "-editor.action.indentLines",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+shift+f8",
                "command": "highlightwords.removeAllHighlights"
            },
            {
                "key": "alt+oem_2",
                "command": "-gitlens.gitCommands",
                "when": "!gitlens:disabled && config.gitlens.keymap == 'alternate'"
            },
            {
                "key": "ctrl+shift+oem_2",
                "command": "references-view.findReferences",
                "when": "editorHasReferenceProvider"
            },
            {
                "key": "f4",
                "command": "references-view.next",
                "when": "reference-list.hasResult && references-view.canNavigate"
            },
            {
                "key": "ctrl+n",
                "command": "editor.action.wordHighlight.next",
                "when": "editorTextFocus && hasWordHighlights"
            },
            {
                "key": "shift+f7",
                "command": "-editor.action.wordHighlight.prev",
                "when": "editorTextFocus && hasWordHighlights"
            },
            {
                "key": "shift+f4",
                "command": "references-view.prev",
                "when": "reference-list.hasResult && references-view.canNavigate"
            },
            {
                "key": "ctrl+alt+k",
                "command": "bookmarks.toggle",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+k",
                "command": "-bookmarks.toggle",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+n",
                "command": "editor.action.nextSelectionMatchFindAction",
                "when": "editorFocus"
            },
            {
                "key": "ctrl+shift+n",
                "command": "editor.action.previousSelectionMatchFindAction",
                "when": "editorFocus"
            },
            {
                "key": "ctrl+alt+n",
                "command": "workbench.action.files.newUntitledFile"
            },
            {
                "key": "alt+oem_2",
                "command": "editor.action.commentLine",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+oem_6",
                "command": "editor.action.jumpToBracket",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+j",
                "command": "workbench.action.gotoLine"
            },
            {
                "key": "ctrl+oem_2",
                "command": "workbench.action.findInFiles"
            },
            {
                "key": "ctrl+shift+g",
                "command": "-workbench.view.scm",
                "when": "workbench.scm.active"
            },
            {
                "key": "ctrl+g",
                "command": "search.action.focusNextSearchResult",
                "when": "hasSearchResult || inSearchEditor"
            },
            {
                "key": "ctrl+shift+g",
                "command": "search.action.focusPreviousSearchResult",
                "when": "hasSearchResult || inSearchEditor"
            },
            {
                "key": "ctrl+oem_period",
                "command": "editor.action.revealDefinition",
                "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
            },
            {
                "key": "ctrl+oem_comma",
                "command": "workbench.action.navigateBack"
            }
        ]
    }
}
